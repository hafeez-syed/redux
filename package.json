{
  "name": "reduxstagram",
  "version": "1.0.0",
  "description": ":) ",
  "scripts": {
    "build-css": "stylus -c src/styles/stylus/ --out src/styles/css",
    "watch-css": "npm run build-css && stylus -c -w src/styles/stylus/ --out src/styles/css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "serve": "serve -s ./build/reduxstagram",
    "prod": "react-scripts build && sh script.sh reduxstagram",
    "deploy-manual": "aws s3 cp --recursive build/ s3://hafeezsyed.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hafeez-syed/redux/tree/master/v2"
  },
  "homepage": "/reduxstagram/",
  "author": "Hafeez Syed",
  "license": "MIT",
  "dependencies": {
    "css-loader": "2.0.0",
    "eslint": "5.6.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.0.1",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-scripts": "2.1.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.3",
    "redux": "^3.5.2",
    "stylus": "0.54.5",
    "webpack": "4.19.1"
  },
  "devDependencies": {
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.5",
    "react-addons-test-utils": "^15.0.2",
    "serve": "10.1.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
